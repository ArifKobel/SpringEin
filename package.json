{
  "name": "SpringEin",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @SpringEin/backend dev",
    "dev:setup": "turbo -F @SpringEin/backend dev:setup"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "pnpm": {
    "overrides": {
      "@react-email/render": "1.1.2"
    }
  },
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@react-email/render": "^1.2.0"
  }
}
